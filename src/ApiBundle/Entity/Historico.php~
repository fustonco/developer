<?php

namespace ApiBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Historico
 */
class Historico
{
    /**
     * @var string
     */
    private $codigo;

    /**
     * @var \DateTime
     */
    private $dataPassagem;

    /**
     * @var integer
     */
    private $id;

    /**
     * @var \ApiBundle\Entity\Funcionario
     */
    private $idde;

    /**
     * @var \ApiBundle\Entity\Mensagem
     */
    private $idmensagem;

    /**
     * @var \ApiBundle\Entity\TipoHistorico
     */
    private $tipoHistorico;

    /**
     * @var \ApiBundle\Entity\Funcionario
     */
    private $idpara;

    /**
     * @var \ApiBundle\Entity\Pedido
     */
    private $idpedido;


    /**
     * Set codigo
     *
     * @param string $codigo
     * @return Historico
     */
    public function setCodigo($codigo)
    {
        $this->codigo = $codigo;

        return $this;
    }

    /**
     * Get codigo
     *
     * @return string 
     */
    public function getCodigo()
    {
        return $this->codigo;
    }

    /**
     * Set dataPassagem
     *
     * @param \DateTime $dataPassagem
     * @return Historico
     */
    public function setDataPassagem($dataPassagem)
    {
        $this->dataPassagem = $dataPassagem;

        return $this;
    }

    /**
     * Get dataPassagem
     *
     * @return \DateTime 
     */
    public function getDataPassagem()
    {
        return $this->dataPassagem;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idde
     *
     * @param \ApiBundle\Entity\Funcionario $idde
     * @return Historico
     */
    public function setIdde(\ApiBundle\Entity\Funcionario $idde = null)
    {
        $this->idde = $idde;

        return $this;
    }

    /**
     * Get idde
     *
     * @return \ApiBundle\Entity\Funcionario 
     */
    public function getIdde()
    {
        return $this->idde;
    }

    /**
     * Set idmensagem
     *
     * @param \ApiBundle\Entity\Mensagem $idmensagem
     * @return Historico
     */
    public function setIdmensagem(\ApiBundle\Entity\Mensagem $idmensagem = null)
    {
        $this->idmensagem = $idmensagem;

        return $this;
    }

    /**
     * Get idmensagem
     *
     * @return \ApiBundle\Entity\Mensagem 
     */
    public function getIdmensagem()
    {
        return $this->idmensagem;
    }

    /**
     * Set tipoHistorico
     *
     * @param \ApiBundle\Entity\TipoHistorico $tipoHistorico
     * @return Historico
     */
    public function setTipoHistorico(\ApiBundle\Entity\TipoHistorico $tipoHistorico = null)
    {
        $this->tipoHistorico = $tipoHistorico;

        return $this;
    }

    /**
     * Get tipoHistorico
     *
     * @return \ApiBundle\Entity\TipoHistorico 
     */
    public function getTipoHistorico()
    {
        return $this->tipoHistorico;
    }

    /**
     * Set idpara
     *
     * @param \ApiBundle\Entity\Funcionario $idpara
     * @return Historico
     */
    public function setIdpara(\ApiBundle\Entity\Funcionario $idpara = null)
    {
        $this->idpara = $idpara;

        return $this;
    }

    /**
     * Get idpara
     *
     * @return \ApiBundle\Entity\Funcionario 
     */
    public function getIdpara()
    {
        return $this->idpara;
    }

    /**
     * Set idpedido
     *
     * @param \ApiBundle\Entity\Pedido $idpedido
     * @return Historico
     */
    public function setIdpedido(\ApiBundle\Entity\Pedido $idpedido = null)
    {
        $this->idpedido = $idpedido;

        return $this;
    }

    /**
     * Get idpedido
     *
     * @return \ApiBundle\Entity\Pedido 
     */
    public function getIdpedido()
    {
        return $this->idpedido;
    }
}
